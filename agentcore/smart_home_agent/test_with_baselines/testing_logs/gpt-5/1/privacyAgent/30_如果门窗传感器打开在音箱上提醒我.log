            # 查询失败，忽略该设备，继续检查下一个
            continue

        # 基本的返回值合法性判断
        if not state_obj or not isinstance(state_obj, dict):
            continue
        # 期望包含state字段
        current_state = state_obj.get("state")
        if current_state is None:
            continue

        # 将状态标准化为字符串小写进行判断
        if isinstance(current_state, str):
            normalized = current_state.strip().lower()
        else:
            # 兼容布尔或数值
            normalized = str(current_state).strip().lower()

        # 对于binary_sensor：on表示打开，off表示关闭
        # 同时兼容可能的"open"/True/1等语义
        if normalized in {"on", "open", "true", "1"}:
            return True

    # 若所有传感器均未检测到打开状态
    return False
