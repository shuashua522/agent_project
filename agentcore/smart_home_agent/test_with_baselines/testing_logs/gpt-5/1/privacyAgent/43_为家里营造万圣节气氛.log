    motion_event_entity = 'event.xiaomi_cn_blt_3_1ftnm7360c800_pir1_motion_detected_e_2_1008'
    illumination_entity = 'sensor.isa_cn_blt_3_1md0u6qht0k00_dw2hl_illumination_p_2_1'

    # 结果默认值
    motion_detected = False
