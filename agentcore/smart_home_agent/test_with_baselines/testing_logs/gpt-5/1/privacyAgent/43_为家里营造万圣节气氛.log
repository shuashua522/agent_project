        if not isinstance(motion_state_obj, dict) or not isinstance(light_state_obj, dict):
            return False

        # 光照条件：必须为“弱”
        light_state = light_state_obj.get("state")
        is_light_weak = str(light_state).strip() == "弱"

        # 移动检测条件：事件时间戳需在阈值秒内
        motion_recent = False
        motion_state_value = motion_state_obj.get("state")
