CACHE_FILE = "_binary_sensor_door_trigger_cache.json"
ENTITY_ID = "binary_sensor.isa_cn_blt_3_1md0u6qht0k00_dw2hl_contact_state_p_2_2"

def check_door_opened_once():
    """
    Return True only when the binary sensor transitions from 'off' to 'on'.
    Triggers once per state change using a local cache of last_state and last_changed.
    """
    # Load previous cache
    prev = {"last_state": None, "last_changed": None}
    if os.path.exists(CACHE_FILE):
        try:
            with open(CACHE_FILE, "r", encoding="utf-8") as f:
                prev = json.load(f)
        except Exception:
            prev = {"last_state": None, "last_changed": None}
    
    # Fetch current state via provided API function
    try:
        state_obj = get_states_by_entity_id(entity_id=ENTITY_ID)
    except NameError:
        # API function not available in this interpreter environment
        return False
    
    # Validate expected keys
    if not isinstance(state_obj, dict) or "state" not in state_obj or "last_changed" not in state_obj or "entity_id" not in state_obj:
        return False
    
    current_state = state_obj["state"]
    current_changed = state_obj["last_changed"]
    
    # Check if transition off -> on happened since last check
    transitioned_off_to_on = (prev.get("last_state") == "off" and current_state == "on" and prev.get("last_changed") != current_changed)
    
    # Update cache
    try:
        with open(CACHE_FILE, "w", encoding="utf-8") as f:
            json.dump({"last_state": current_state, "last_changed": current_changed}, f)
    except Exception:
        pass
    
    return transitioned_off_to_on

