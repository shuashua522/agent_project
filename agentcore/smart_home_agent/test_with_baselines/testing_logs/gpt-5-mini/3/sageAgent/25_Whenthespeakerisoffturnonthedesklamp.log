import os
import sys
import requests

def is_speaker_off():
    """
    Returns True if the primary/default speaker is OFF, False otherwise.
    Heuristics:
      1) Look for entity with friendly_name 'Speaker' or entity_id ending with '.speaker'.
      2) Else search all media_player.* entities and pick the best match by friendly_name or attributes.
      3) Query /api/states/<entity_id> and return True if state == 'off'.
    Requires HA_URL and HA_TOKEN environment variables.
    """
    HA_URL = os.environ.get('HA_URL')
    HA_TOKEN = os.environ.get('HA_TOKEN')
    if not HA_URL or not HA_TOKEN:
        raise RuntimeError("Please set HA_URL and HA_TOKEN environment variables.")

    headers = {
        "Authorization": f"Bearer {HA_TOKEN}",
        "Content-Type": "application/json",
    }
    # Step 1: get all entities
    resp = requests.get(f"{HA_URL.rstrip('/')}/api/states", headers=headers, timeout=10)
    resp.raise_for_status()
    states = resp.json()  # list of state objects

    # Try exact friendly_name match or entity_id match first
    candidate = None
    for s in states:
        eid = s.get('entity_id', '')
        attrs = s.get('attributes', {}) or {}
        fname = str(attrs.get('friendly_name') or '').strip()
        if fname.lower() == 'speaker' or eid.lower().endswith('.speaker') or eid.lower() == 'media_player.speaker':
            candidate = eid
            break

    # Step 2: if not found, search media_player domain entities with heuristics
    if not candidate:
        media_players = [s for s in states if s.get('entity_id', '').startswith('media_player.')]
        # Prefer obvious matches
        for s in media_players:
            eid = s['entity_id']
            attrs = s.get('attributes', {}) or {}
            fname = str(attrs.get('friendly_name') or '').lower()
            # Heuristic checks: name contains 'speaker' or attributes mark it as default/primary
            if 'speaker' in fname or attrs.get('is_primary') or attrs.get('default') is True or attrs.get('device_class') == 'speaker':
                candidate = eid
                break
        # If still none, fall back to first media_player (device may be default)
        if not candidate and media_players:
            candidate = media_players[0]['entity_id']

    if not candidate:
        # No media_player found — cannot determine; return False by default
        print("Warning: No speaker/media_player entity found.", file=sys.stderr)
        return False

    # Step 3: fetch the state for the chosen entity
    resp2 = requests.get(f"{HA_URL.rstrip('/')}/api/states/{candidate}", headers=headers, timeout=10)
    if resp2.status_code == 404:
        print(f"Warning: Entity {candidate} not found when fetching state.", file=sys.stderr)
        return False
    resp2.raise_for_status()
    state_obj = resp2.json()
    state = state_obj.get('state', '').lower()
    # Step 4: check if 'off'
    return state == 'off'

# Example call:
print(is_speaker_off())

