def check_outdoor_nice():
    """
    Check whether outdoor weather is 'nice' at home:
      - Not raining (based on weather entity state/condition)
      - Temperature roughly between 15°C and 25°C (inclusive)
    Returns:
      - True if nice (recommend opening windows)
      - False otherwise
    This function queries the weather entity 'weather.forecast_wo_de_jia'.
    """
    # Query the weather entity
    resp = functions.get_states_by_entity_id({"entity_id": "weather.forecast_wo_de_jia"})
    if not resp or "entity_id" not in resp:
        # Cannot find entity -> treat as not satisfied
        return False

    state = resp.get("state", "") or ""
    attrs = resp.get("attributes", {}) or {}

    # Determine temperature value and unit
    temp = None
    unit = None
    # Common attribute names to check
    if "temperature" in attrs and attrs["temperature"] is not None:
        temp = attrs["temperature"]
        unit = attrs.get("temperature_unit")
    elif "current_temperature" in attrs and attrs["current_temperature"] is not None:
        temp = attrs["current_temperature"]
        unit = attrs.get("temperature_unit")
    # If still no temp, try forecast list first item (if present)
    if temp is None and isinstance(attrs.get("forecast"), list) and len(attrs["forecast"]) > 0:
        f0 = attrs["forecast"][0]
        temp = f0.get("temperature") or f0.get("templow") or f0.get("temp")
        unit = attrs.get("temperature_unit") or f0.get("temperature_unit")

    # If temp still missing, cannot decide -> treat as not satisfied
    if temp is None:
        return False

    # Normalize unit and convert to Celsius if needed
    unit_str = (unit or "").strip()
    try:
        temp_val = float(temp)
    except Exception:
        return False

    temp_c = temp_val
    # handle common Fahrenheit markers
    if unit_str in ("°F", "F", "fahrenheit", "°f"):
        temp_c = (temp_val - 32.0) * 5.0 / 9.0
    # If unit is in °C or C or absent, we assume Celsius
    # Round to one decimal for readability (but keep numeric)
    temp_c = round(temp_c, 1)

    # Determine precipitation / raining by checking state and attributes
    raining = False
    state_lower = state.lower()
    rain_keywords = ["rain", "shower", "drizzle", "sleet", "thunder", "storm", "snow"]
    for kw in rain_keywords:
        if kw in state_lower:
            raining = True
            break

    # Additional: check forecast precipitation probability if present (nonzero -> caution)
    if not raining:
        # check attributes.forecast[0].precipitation_probability or precipitation
        forecast = attrs.get("forecast")
        if isinstance(forecast, list) and len(forecast) > 0:
            f0 = forecast[0]
            prob = f0.get("precipitation_probability") or f0.get("precipitation") or f0.get("chance_of_rain")
            try:
                if prob is not None:
                    # treat non-zero or >=10% as possible rain (be conservative)
                    p = float(prob)
                    if p > 0:
                        raining = True
            except Exception:
                pass

    # Evaluate temperature criterion: roughly between 15°C and 25°C inclusive
    temp_ok = (temp_c >= 15.0) and (temp_c <= 25.0)

    # Condition is satisfied if not raining and temp_ok
    is_nice = (not raining) and temp_ok
    return is_nice

