TOKEN = "YOUR_LONG_LIVED_ACCESS_TOKEN"
ENTITY_ID = "sun.sun"

def _iso_to_dt_local(iso_ts):
    # Normalize trailing Z to +00:00 so fromisoformat accepts it
    if iso_ts.endswith("Z"):
        iso_ts = iso_ts[:-1] + "+00:00"
    # If no offset present, fromisoformat returns naive dt; assume UTC in that case
    try:
        dt = datetime.fromisoformat(iso_ts)
    except Exception:
        # Fallback parse for timestamps without separators
        dt = datetime.strptime(iso_ts, "%Y-%m-%dT%H:%M:%S.%f")
        dt = dt.replace(tzinfo=timezone.utc)
    if dt.tzinfo is None:
        dt = dt.replace(tzinfo=timezone.utc)
    # Convert to local timezone
    return dt.astimezone()

def check_tomorrow_at_07_using_sun():
    """
    Returns True if the sun.sun entity's last_updated time, converted to local timezone,
    equals tomorrow at 07:00:00 local time (exact second). Otherwise returns False.
    """
    url = f"{HOME_ASSISTANT_URL}/api/states/{ENTITY_ID}"
    headers = {
        "Authorization": f"Bearer {TOKEN}",
        "Content-Type": "application/json",
    }
    resp = requests.get(url, headers=headers, timeout=10)
    resp.raise_for_status()
    state_obj = resp.json()
    ts = state_obj.get("last_updated") or state_obj.get("last_changed")
    if not ts:
        return False
    local_dt = _iso_to_dt_local(ts).replace(microsecond=0)
    # Compute tomorrow relative to the local date of the timestamp we received
    tomorrow_date = local_dt.date() + timedelta(days=1)
    target = datetime(tomorrow_date.year, tomorrow_date.month, tomorrow_date.day,
                      7, 0, 0, tzinfo=local_dt.tzinfo)
    return local_dt == target

