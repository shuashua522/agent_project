def is_any_contact_sensor_open():
    \"\"\"Return True if any binary_sensor whose attributes.device_class is
    'door', 'window' or 'opening' has state 'on' or 'open'.
    Uses the system snapshot returned by get_all_entity_id().
    \"\"\"
    OPEN_STATES = {'on', 'open'}
    DEVICE_CLASSES = {'door', 'window', 'opening'}

    # 获取所有实体（工具：get_all_entity_id）
    entities = functions.get_all_entity_id()

    if not isinstance(entities, list):
        # 若返回格式异常，视为无触发
        return False

    for ent in entities:
        eid = ent.get('entity_id', '')
        if not isinstance(eid, str) or not eid.startswith('binary_sensor.'):
            continue
        attrs = ent.get('attributes', {}) or {}
        device_class = attrs.get('device_class')
        if device_class in DEVICE_CLASSES:
            state = ent.get('state')
            if state in OPEN_STATES:
                return True
    return False

